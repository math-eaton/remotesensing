{
  "name": "vite-vanilla-js-template",
  "description": "ViteJS starter template for vanilla JavaScript with some basic dependencies.",
  "version": "1.0.0",
  "homepage": "https://math-eaton.github.io/remotesensing/",
  "type": "module",
  "keywords": [
    "vanilla",
    "javascript",
    "template",
    "boilerplate",
    "boilerplate-template",
    "postcss",
    "eslint",
    "prettier",
    "scaffold",
    "scaffold-template",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template"
  ],
  "scripts": {
    "dev": "vite --host --open",
    "start:server": "node server.js",
    "start:dev": "concurrently \"npm run dev\" \"npm run start:server\"",
    "lint": "eslint --ext .js ./",
    "lint:fix": "eslint --fix --ext .js ./",
    "format": "prettier --write \"./**/*.{js,jsx,json,css}\"",
    "build": "vite build",
    "preview": "vite preview --open",
    "buildpreview": "vite build && vite preview --open",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.35",
    "postcss-nesting": "^12.0.3",
    "prettier": "^3.2.5",
    "vite": "5.2.8",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "@serialport/parser-readline": "^12.0.0",
    "convex-hull": "^1.0.3",
    "delaunator": "^5.0.1",
    "eslint": "^8.57.0",
    "geojson": "^0.5.0",
    "geojson-vt": "^3.2.1",
    "proj4": "^2.10.0",
    "serialport": "^12.0.0",
    "simplex-noise": "^4.0.1",
    "the-new-css-reset": "^1.11.2",
    "three": "^0.162.0",
    "websocket": "^1.0.34",
    "ws": "^8.16.0"
  }
}
